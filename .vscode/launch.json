{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Google Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/tests/Debug/test_calculator.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "gdb",
            "setupCommands": [],
            "logging": {
                "trace": true,
                "traceResponse": true
            },
            "windows": {
                "program": "${workspaceFolder}/build/tests/Debug/test_calculator.exe",
                "miDebuggerPath": "C:/mingw/bin/gdb.exe"
            },
            "osx": {
                "miDebuggerPath": "/usr/local/bin/gdb"
            },
            "linux": {
                "miDebuggerPath": "/usr/bin/gdb"
            }
        }
    ]
}
